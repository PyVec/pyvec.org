{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "role": {
      "type": "string"
    },
    "github": {
      "type": ["string", "null"]
    },
    "twitter": {
      "type": ["string", "null"]
    },
    "linkedin": {
      "type": ["string", "null"]
    },
    "avatar_filename": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "name",
    "role",
    "avatar_filename"
  ]
}
